#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n,T=1,N=2,S=5,E=4,W=3,B=6;
    string direction;
    while(cin>>n && n)
    {
        for(int i=0;i<n;i++)
        {
            cin>>direction;
            if(direction=="north")
            {
                swap(T,S);
                swap(B,S);
                swap(B,N);
            }
            if(direction=="east")
            {
                swap(T,W);
                swap(B,W);
                swap(B,E);
            }
            if(direction=="west")
            {
                swap(T,E);
                swap(B,E);
                swap(B,W);
            }
            if(direction=="south")
            {
                swap(T,N);
                swap(B,N);
                swap(B,S);
            }
        }
        cout<<T<<endl;

        T=1,N=2,S=5,E=4,W=3,B=6;
    }
}
